
let fav_fruit = ["Mango", "Orange", "Grapes"]


if(fav_fruit.includes("Banana")){
    console.log("banana is my favourite")
}else{
    console.log("Banana is not my favourite fruit")
}

if(fav_fruit.includes("Mango")){
    console.log("Mango is my favourite")
}else{
    console.log("Mango is not my favourite fruit")
}

if(fav_fruit.includes("Orange")){
    console.log("Orange is my favourite")
}else{
    console.log("Orange is not my favourite fruit")
}
if(fav_fruit.includes("Grapes")){
    console.log("Grapes is my favourite")
}else{
    console.log("Grapes is not my favourite fruit")
}
if(fav_fruit.includes("Apples")){
    console.log("Apples is my favourite")
}else{
    console.log("Apples is not my favourite fruit")
}
